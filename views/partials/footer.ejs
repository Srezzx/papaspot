<script>
    $(window).scroll(function () {
        if ($(window).scrollTop() >= 0 && $(window).scrollTop() < 100) {
            $('.navbar').css('background', 'rbg(0,27,66,0.85)');
            $('.navbar').css('display', 'block');
        } else if ($(window).scrollTop() >= 100 && $(window).scrollTop() < 450) {
            $('.navbar').css('display', 'none');
        } else if ($(window).scrollTop() >= 405) {
            $('.navbar').css('background', '#001b42');
            $('.navbar').css('display', 'block');
        } else {
            $('.navbar').css('background', 'transparent');
            $('.navbar').css('display', 'block');
        }
    });
</script>
